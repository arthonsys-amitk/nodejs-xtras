							for(var i = 0; i < service.all_categories.length; i++) {							
							%>
							<option value="<%=service.all_categories[i]._id%>" ><%=service.all_categories[i].category_name%></option>
							<%
							}
						}
					  %>
					  </select>
                    </div>
                  </div>
