								<div class="input-group-addon">
								  <i class="fa fa-clock-o"></i>
								</div>
								<input type="text" class="form-control timepicker" id="weekday_start_time" name="weekday_start_time" value="<%=service.weekday_start_time%>">
								
							  </div>
						  </div>
						</div>
				 </div>
				  
				  <div class="bootstrap-timepicker">
