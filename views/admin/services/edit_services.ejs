					<%
						var avl_mon = (service.available_monday != 0 && service.available_monday != "0")? "selected" : "";
						var avl_tue = (service.available_tuesday != 0 && service.available_tuesday != "0")? "selected" : "";
						var avl_wed = (service.available_wednesday != 0 && service.available_wednesday != "0")? "selected" : "";
						var avl_thu = (service.available_thursday != 0 && service.available_thursday != "0")? "selected" : "";
						var avl_fri = (service.available_friday != 0 && service.available_friday != "0")? "selected" : "";
						var avl_sat = (service.available_saturay != 0 && service.available_saturday != "0")? "selected" : "";
