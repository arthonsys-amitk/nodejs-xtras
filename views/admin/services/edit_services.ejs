								var kms = "";
								var miles = "";
								if(service.service_radius_units == "Km" || service.service_radius_units == "Kms" ) {
									kms = "selected";
								} else {
									miles = "selected";
								}
							%>
							<option value="Km" <%=kms%>>Km</option>
							<option value="Miles" <%=miles%>>Miles</option>
						  </select>
						  </div>
                  </div>
				  
				  <%
